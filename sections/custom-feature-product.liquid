    <style>
      .wenexus-custom-feature-product{
      	  /*padding-top: 150px;*/
      }
        .wenexus-custom-feature-product .single-feature-icon svg{
            width:60px;
            height: 60px;
            margin: 0 auto;
            fill: #252525;
        }
        .wenexus-custom-feature-product .section-wrapper{
            width: 100%;
            margin: 0 auto;
          	padding: 80px 0;
          	{% if section.settings.background_color %}
          	background: {{section.settings.background_color}};
          	{%endif%}
        }
       .wenexus-custom-feature-product .container{
            max-width: 1300px;
            width: 90%;
            margin:  0 auto;
        }
        .wenexus-custom-feature-product .section-title-subtitle{
            text-align: center;
        }
        .wenexus-custom-feature-product .section-title-subtitle .title{
/*             font-family: Poppins, sans-serif; */
            font-size: 48px;
            font-weight: 600;
            line-height: 1.2em;
            position: relative;
            z-index: 1;
            /*margin: 0 0 45px;*/
            padding: 0;
            color: rgb(0, 0, 0);
          	{% if section.settings.color %}
          	color: {{section.settings.color}};
          	{%endif%}
        }
       .wenexus-custom-feature-product .section-title-subtitle p {
            font-size: 24px;
            font-weight: 300;
            line-height: 1.69em;
            color: rgb(165, 165, 165);
         	{% if section.settings.color %}
          	color: {{section.settings.color}};
          	{%endif%}
            margin-bottom: 15px;
        }
       .wenexus-custom-feature-product .section-features{
            display: flex;
            justify-content: space-between;
            width: 90%;
            margin: 45px auto 0 auto;
            flex-wrap: wrap;
            gap: 10px;
        }
        .wenexus-custom-feature-product .section-features .single-feature{
            text-align: center;
            flex: 1;
        }
        .wenexus-custom-feature-product .section-features .single-feature .single-feature-icon{
            width: 98px;
            height: 98px;
            background: linear-gradient(0deg, #4cff6c 0%, #a4f3b3 100%);
            border-radius: 20px;
            padding: 20px;
            margin: 0 auto 10px auto;
        }
        .wenexus-custom-feature-product .product-img{
            text-align: center;
            margin-top: 30px;
            max-width: 600px;
            margin: 0 auto;
        } 
        .wenexus-custom-feature-product .product-img img {
            display: block;
            /*margin: 0 auto;*/
            width: 100%;
        }
        @media screen and (max-width:  768px) {
             .wenexus-custom-feature-product .section-title-subtitle .title{
                font-size: 40px;
             }
         } 
    </style>
